	<queryString>
		<![CDATA[SELECT t1.folio,t1.proveedor,t1.factura_id,t1.factura,t1.unidad_embarcar_id,t1.unidad_embarcar,t1.metodo_pago,t1.moneda,t1.fecha_requisicion,t1.folio_requisicion,
t1.fecha_elaboracion,t1.comentarios_revision,t1.observaciones,t1.importe,
t1.descuento, t1.subtotal,t1.iva,t1.total,t1.porc_iva,t1.importe_letra,
t1.solicita,t1.recibe,t1.autoriza,t1.estado_doc,t1.fecha_autoriza,t1.firma_autoriza,t1.fecha_solicita,t1.firma_solicita,t1.fecha_recibe,t1.firma_recibe,
t1.fecha_solicita,t1.firma_solicita,t1.fecha_recibe,t1.firma_recibe,
t4.no_parte,t4.descripcion,t4.precio_unitario,t4.cantidad,t4.importe as det_importe,t4.descuento as det_descuento,t4.subtotal as det_subtotal,t4.iva as det_iva,t4.total as del_total,
t2.domicilio, t2.ciudad, t2.telefono,t2.empresa_rfc,
t3.domicilio as domicilio_emb, t3.ciudad as ciudad_emb, t3.telefono as telefono_emb, t3.empresa_rfc as rfc_emb

from orden_de_compra t1
INNER JOIN unidad_negocio t2 on t1.factura_id = t2.id
INNER JOIN unidad_negocio t3 on t1.unidad_embarcar_id = t3.id
INNER JOIN orden_de_compra_det t4 on t1.requisicion_id = t4.documento_id
where t1.id = $P{id}]]>
